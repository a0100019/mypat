import java.io.*
import java.util.*

fun main() {
    val br = BufferedReader(InputStreamReader(System.`in`))

    repeat(3) { // 입력 줄 수를 알고 있다면 3번 반복
        val st = StringTokenizer(br.readLine())
        val a = st.nextToken().toInt()
        val b = st.nextToken().toInt()
        println("a = $a, b = $b") // 확인용 출력
    }
}